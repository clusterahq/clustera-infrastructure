# AWS Credentials (for S3 backend state storage)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1

# Pulumi Configuration
PULUMI_ACCESS_TOKEN=pul-xxxxxxxxxxxxxxxxxxxx
PULUMI_CONFIG_PASSPHRASE=your-secure-passphrase

# Aiven Configuration
AIVEN_TOKEN=your_aiven_api_token

# GCP Configuration
GOOGLE_CREDENTIALS=/path/to/gcp-service-account-key.json
# OR use GOOGLE_APPLICATION_CREDENTIALS
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/gcp-service-account-key.json

# Optional: Override GCP project for commands
# GOOGLE_PROJECT=your-gcp-project-id
